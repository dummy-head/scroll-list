{
  "name": "vue-virtual-scroll-list",
  "version": "1.3.2",
  "description": "A vue component support big amount data list with high scroll performance.",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint index.js",
    "ci": "npm run lint && npm run test",
    "dev": "npm run server & npm run demo:dev",
    "server": "node node_modules/@tangbc/tiny-node-server",
    "demo:dev": "cross-env NODE_ENV=development webpack --config ./demos/webpack.conf.js",
    "demo:build": "cross-env NODE_ENV=production webpack --config ./demos/webpack.conf.js"
  },
  "keywords": [
    "vue",
    "component",
    "big-data",
    "big-list",
    "scroll-list",
    "virtual-list"
  ],
  "tinyNodeServer": {
    "root": "./",
    "port": 8088,
    "log": 0,
    "suffix": "/demos/item-mode"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tangbc/vue-virtual-scroll-list"
  },
  "author": "TANG",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@tangbc/tiny-node-server": "^1.0.4",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.7.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.7.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
