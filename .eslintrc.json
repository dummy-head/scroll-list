{
    "extends": [
        "standard",
        "eslint:recommended",
        "plugin:es5/no-es2015",
        "plugin:es5/no-es2016"
    ],
    "env": {
        "browser": true,
        "amd": true
    },
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "arrow-parens": 0,
        "generator-star-spacing": 0,
        "no-trailing-spaces": [0, { "skipBlankLines": true }]
    }
}